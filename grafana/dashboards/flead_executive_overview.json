    {
      "id": 1,
      "type": "stat",
      "title": "",
      "description": "Overall system health score based on anomaly rate, model accuracy, and data freshness",
      "datasource": "FLEAD-TimescaleDB",
      "gridPos": { "h": 5, "w": 4, "x": 0, "y": 1 },
      "targets": [
        {
          "refId": "A",
          "format": "time_series",
          "rawSql": "WITH health AS (\n  SELECT \n    GREATEST(0, 100 - (SELECT COUNT(*) * 2 FROM anomalies WHERE ts > now() - interval '1 hour' AND severity = 'critical')) as anomaly_health,\n    COALESCE((SELECT accuracy * 100 FROM federated_models ORDER BY created_at DESC LIMIT 1), 50) as model_health,\n    CASE WHEN EXISTS(SELECT 1 FROM local_models WHERE created_at > now() - interval '1 hour') THEN 100 ELSE 50 END as freshness\n)\nSELECT now() AS time, ROUND((anomaly_health + model_health + freshness) / 3) AS value FROM health;"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "#FF4136", "value": null },
              { "color": "#FF851B", "value": 50 },
              { "color": "#2ECC40", "value": 80 }
            ]
          },
          "mappings": [],
          "color": { "mode": "thresholds" }
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "value_and_name",
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "text": { "titleSize": 14, "valueSize": 48 }
      },
      "title": "üè• System Health"
