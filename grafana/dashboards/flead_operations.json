            "showPoints": "always"
          }
        },
        "overrides": [
          { "matcher": { "id": "byName", "options": "Devices Aggregated" }, "properties": [{ "id": "color", "value": { "fixedColor": "#00D4FF", "mode": "fixed" } }] }
        ]
      },
      "options": {
        "legend": { "showLegend": true, "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 22,
      "type": "timeseries",
      "title": "üìä Stream Analysis Events",
      "description": "Real-time stream processing results",
      "datasource": "FLEAD-TimescaleDB",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 16 },
      "targets": [
        {
          "refId": "A",
          "format": "time_series",
          "rawSql": "SELECT time_bucket('5 minutes', timestamp) AS time, COUNT(*) AS \"Events\", SUM(CASE WHEN is_anomaly THEN 1 ELSE 0 END) AS \"Anomalies\" FROM stream_analysis_results WHERE $__timeFilter(timestamp) GROUP BY 1 ORDER BY 1;"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 20,
            "showPoints": "auto"
          }
        }
      },
      "options": {
        "legend": { "showLegend": true, "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 103,
      "type": "row",
      "title": "üìã Dashboard Metrics",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 24 },
