    {
      "id": 1,
      "type": "stat",
      "title": "üìä Total IoT Msgs",
      "description": "Total IoT messages in database",
      "datasource": "FLEAD-TimescaleDB",
      "gridPos": { "h": 5, "w": 4, "x": 0, "y": 1 },
      "targets": [
        {
          "refId": "A",
          "format": "time_series",
          "rawSql": "SELECT now() AS time, COUNT(*) AS value FROM iot_data;"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "msg/s",
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "#FF4136", "value": null },
              { "color": "#FF851B", "value": 1 },
              { "color": "#2ECC40", "value": 5 }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "colorMode": "background_solid",
        "graphMode": "area",
        "justifyMode": "center",
        "textMode": "value_and_name"
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "üì° Device Count",
      "description": "Total unique devices",
      "datasource": "FLEAD-TimescaleDB",
      "gridPos": { "h": 5, "w": 4, "x": 4, "y": 1 },
      "targets": [
        {
          "refId": "A",
          "format": "time_series",
          "rawSql": "SELECT now() AS time, COUNT(DISTINCT device_id) AS value FROM iot_data;"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "#FF4136", "value": null },
              { "color": "#FF851B", "value": 100 },
              { "color": "#2ECC40", "value": 500 }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "colorMode": "background_solid",
        "graphMode": "area",
        "justifyMode": "center"
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "üîî Alerts/5min",
      "description": "Anomalies detected in last 5 minutes",
      "datasource": "FLEAD-TimescaleDB",
      "gridPos": { "h": 5, "w": 4, "x": 8, "y": 1 },
      "targets": [
        {
          "refId": "A",
          "format": "time_series",
          "rawSql": "SELECT now() AS time, COUNT(*) AS value FROM anomalies WHERE ts > now() - interval '5 minutes';"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "#2ECC40", "value": null },
              { "color": "#FFDC00", "value": 3 },
              { "color": "#FF851B", "value": 10 },
              { "color": "#FF4136", "value": 25 }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "colorMode": "background_solid",
        "graphMode": "area",
        "justifyMode": "center"
      }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "‚è±Ô∏è Since Anomaly",
      "description": "Time since last anomaly",
      "datasource": "FLEAD-TimescaleDB",
      "gridPos": { "h": 5, "w": 4, "x": 12, "y": 1 },
      "targets": [
        {
          "refId": "A",
          "format": "time_series",
          "rawSql": "SELECT now() AS time, EXTRACT(EPOCH FROM (now() - MAX(ts))) AS value FROM anomalies;"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "#2ECC40", "value": null },
              { "color": "#FFDC00", "value": 60 },
              { "color": "#FF851B", "value": 300 },
              { "color": "#FF4136", "value": 600 }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "colorMode": "background_solid",
        "graphMode": "none",
        "justifyMode": "center"
      }
    },
    {
      "id": 5,
      "type": "stat",
      "title": "üß† Model Lag",
      "description": "Time since last FL aggregation",
      "datasource": "FLEAD-TimescaleDB",
      "gridPos": { "h": 5, "w": 4, "x": 16, "y": 1 },
      "targets": [
        {
          "refId": "A",
          "format": "time_series",
          "rawSql": "SELECT now() AS time, EXTRACT(EPOCH FROM (now() - COALESCE(MAX(created_at), now() - interval '1 hour'))) AS value FROM federated_models;"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "#2ECC40", "value": null },
              { "color": "#FFDC00", "value": 300 },
              { "color": "#FF851B", "value": 900 },
              { "color": "#FF4136", "value": 1800 }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "colorMode": "background_solid",
        "graphMode": "none",
        "justifyMode": "center"
      }
    },
    {
      "id": 6,
      "type": "gauge",
      "title": "üìà Pipeline Health",
      "description": "Overall pipeline health score",
      "datasource": "FLEAD-TimescaleDB",
      "gridPos": { "h": 5, "w": 4, "x": 20, "y": 1 },
      "targets": [
        {
          "refId": "A",
          "format": "time_series",
          "rawSql": "WITH metrics AS (\n  SELECT\n    CASE WHEN EXISTS(SELECT 1 FROM anomalies WHERE ts > now() - interval '5 minutes') THEN 40 ELSE 0 END +\n    CASE WHEN EXISTS(SELECT 1 FROM local_models WHERE created_at > now() - interval '1 hour') THEN 30 ELSE 0 END +\n    GREATEST(0, 30 - (SELECT COUNT(*) FROM anomalies WHERE ts > now() - interval '15 minutes' AND severity = 'critical')) AS health\n)\nSELECT now() AS time, health AS value FROM metrics;"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "#FF4136", "value": null },
              { "color": "#FF851B", "value": 50 },
              { "color": "#2ECC40", "value": 80 }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      }
    },
    {
      "id": 101,
      "type": "row",
      "title": "üìä Data Ingestion Flow",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 6 },
      "collapsed": false
    },
    {
      "id": 10,
      "type": "timeseries",
      "title": "üìà Anomaly Rate (per minute)",
      "description": "Anomaly detection rate over time",
      "datasource": "FLEAD-TimescaleDB",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 7 },
      "targets": [
        {
          "refId": "A",
          "format": "time_series",
          "rawSql": "SELECT time_bucket('1 minute', ts) AS time, COUNT(*) AS \"Anomalies\" FROM anomalies WHERE $__timeFilter(ts) GROUP BY 1 ORDER BY 1;"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 40,
            "gradientMode": "opacity",
            "showPoints": "never"
          }
        },
        "overrides": [
          { "matcher": { "id": "byName", "options": "Messages" }, "properties": [{ "id": "color", "value": { "fixedColor": "#00D4FF", "mode": "fixed" } }] }
        ]
      },
      "options": {
        "legend": { "showLegend": true, "displayMode": "table", "placement": "right", "calcs": ["sum", "mean", "max", "last"] },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 11,
      "type": "timeseries",
      "title": "üîî Anomaly Detection Rate",
      "description": "Anomalies detected per minute",
      "datasource": "FLEAD-TimescaleDB",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 7 },
      "targets": [
        {
          "refId": "All",
          "format": "time_series",
          "rawSql": "SELECT time_bucket('1 minute', ts) AS time, COUNT(*) AS \"Total\" FROM anomalies WHERE $__timeFilter(ts) GROUP BY 1 ORDER BY 1;"
        },
        {
          "refId": "HIGH",
          "format": "time_series",
          "rawSql": "SELECT time_bucket('1 minute', ts) AS time, COUNT(*) AS \"HIGH\" FROM anomalies WHERE $__timeFilter(ts) AND severity = 'HIGH' GROUP BY 1 ORDER BY 1;"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 30,
            "showPoints": "auto"
          }
        },
        "overrides": [
          { "matcher": { "id": "byName", "options": "Total" }, "properties": [{ "id": "color", "value": { "fixedColor": "#FF851B", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "HIGH" }, "properties": [{ "id": "color", "value": { "fixedColor": "#FF4136", "mode": "fixed" } }, { "id": "custom.fillOpacity", "value": 60 }] }
        ]
      },
      "options": {
        "legend": { "showLegend": true, "displayMode": "table", "placement": "right", "calcs": ["sum", "mean", "max"] },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 102,
      "type": "row",
      "title": "üîÑ Processing Pipeline",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 15 },
      "collapsed": false
    },
    {
      "id": 20,
      "type": "timeseries",
      "title": "üè† Local Model Training Rate",
      "description": "Local models created per 5 minutes",
      "datasource": "FLEAD-TimescaleDB",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 16 },
      "targets": [
        {
          "refId": "A",
