        ]
      },
      "options": {
        "legend": { "showLegend": true, "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 21,
      "type": "timeseries",
      "title": "üß† FL Aggregation Events",
      "description": "Global model updates over time",
      "datasource": "FLEAD-TimescaleDB",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 16 },
      "targets": [
        {
          "refId": "A",
          "format": "time_series",
          "rawSql": "SELECT created_at AS time, num_devices AS \"Devices Aggregated\" FROM federated_models WHERE $__timeFilter(created_at) ORDER BY created_at;"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "points",
