    },
    {
      "id": 30,
      "type": "timeseries",
      "title": "üìà Pipeline KPIs Over Time",
      "description": "Key metrics tracked by the pipeline",
      "datasource": "FLEAD-TimescaleDB",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 25 },
      "targets": [
        {
          "refId": "A",
          "format": "time_series",
          "rawSql": "SELECT timestamp AS time, metric_name, metric_value AS value FROM dashboard_metrics WHERE $__timeFilter(timestamp) AND metric_value > 0 ORDER BY timestamp;"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "stepAfter",
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "auto"
          }
        }
      },
      "options": {
        "legend": { "showLegend": true, "displayMode": "table", "placement": "right", "calcs": ["last", "mean"] },
        "tooltip": { "mode": "multi" }
      },
      "transformations": [
        {
          "id": "renameByRegex",
          "options": { "regex": "value (.*)", "renamePattern": "$1" }
        }
      ]
    },
    {
      "id": 31,
      "type": "table",
      "title": "üìã Current KPI Status",
      "description": "Latest values for all tracked metrics",
      "datasource": "FLEAD-TimescaleDB",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 33 },
      "targets": [
        {
          "refId": "A",
          "format": "table",
          "rawSql": "SELECT DISTINCT ON (metric_name)\n  timestamp AS \"Updated\",\n  metric_name AS \"Metric\",\n  metric_value AS \"Value\",\n  metric_unit AS \"Unit\",\n  COALESCE(device_id, 'system') AS \"Source\"\nFROM dashboard_metrics\nWHERE metric_value > 0\nORDER BY metric_name, timestamp DESC;"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": { "align": "auto", "cellOptions": { "type": "auto" }, "filterable": true }
        },
        "overrides": [
          { "matcher": { "id": "byName", "options": "Updated" }, "properties": [{ "id": "unit", "value": "dateTimeAsLocal" }] },
          { "matcher": { "id": "byName", "options": "Value" }, "properties": [{ "id": "custom.cellOptions", "value": { "type": "color-text" } }, { "id": "color", "value": { "mode": "continuous-BlYlRd" } }] }
        ]
      },
      "options": { "showHeader": true, "cellHeight": "sm" }
    },
    {
      "id": 104,
      "type": "row",
      "title": "üì° Live Data Feeds",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 41 },
      "collapsed": true,
      "panels": [
        {
          "id": 40,
          "type": "table",
          "title": "üîÑ Latest IoT Messages",
          "datasource": "FLEAD-TimescaleDB",
          "gridPos": { "h": 10, "w": 12, "x": 0, "y": 42 },
          "targets": [
            {
              "refId": "A",
              "format": "table",
              "rawSql": "SELECT ts AS \"Time\", device_id AS \"Device\", ROUND(value::numeric, 4) AS \"Value\", CASE WHEN label = 1 THEN '‚ö†Ô∏è' ELSE '‚úÖ' END AS \"Status\" FROM iot_data ORDER BY ts DESC LIMIT 50;"
            }
          ],
          "fieldConfig": {
            "defaults": { "custom": { "align": "auto", "filterable": true } },
            "overrides": [
              { "matcher": { "id": "byName", "options": "Time" }, "properties": [{ "id": "unit", "value": "dateTimeAsLocal" }] }
            ]
          },
          "options": { "showHeader": true, "cellHeight": "sm" }
        },
        {
          "id": 41,
          "type": "table",
          "title": "üîî Latest Anomalies",
          "datasource": "FLEAD-TimescaleDB",
          "gridPos": { "h": 10, "w": 12, "x": 12, "y": 42 },
          "targets": [
            {
              "refId": "A",
              "format": "table",
              "rawSql": "SELECT ts AS \"Time\", device_id AS \"Device\", ROUND(anomaly_score::numeric, 3) AS \"Score\", severity AS \"Severity\" FROM anomalies ORDER BY ts DESC LIMIT 50;"
            }
          ],
          "fieldConfig": {
            "defaults": { "custom": { "align": "auto", "filterable": true } },
            "overrides": [
              { "matcher": { "id": "byName", "options": "Time" }, "properties": [{ "id": "unit", "value": "dateTimeAsLocal" }] },
              { "matcher": { "id": "byName", "options": "Severity" }, "properties": [{ "id": "mappings", "value": [{ "type": "value", "options": { "HIGH": { "color": "red", "text": "üî¥ HIGH" }, "MEDIUM": { "color": "orange", "text": "üü† MED" }, "LOW": { "color": "yellow", "text": "üü° LOW" } } }] }] }
            ]
          },
          "options": { "showHeader": true, "cellHeight": "sm" }
        }
      ]
    }
