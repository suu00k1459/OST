      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 },
      "targets": [
        {
          "refId": "A",
          "format": "time_series",
          "rawSql": "SELECT\n  now() AS time,\n  COUNT(DISTINCT device_id) AS value\nFROM stream_analysis_results\nWHERE timestamp > now() - interval '10 minutes';"
        }
      ],
      "fieldConfig": {
