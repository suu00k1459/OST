    {
      "id": 8,
      "type": "table",
      "title": "Top anomalous devices (last 24h)",
      "datasource": "FLEAD-TimescaleDB",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "targets": [
        {
          "refId": "A",
          "format": "table",
          "rawSql": "SELECT\n  device_id,\n  COUNT(*) AS anomaly_count,\n  MAX(anomaly_confidence) AS max_confidence,\n  MAX(timestamp) AS last_seen\nFROM stream_analysis_results\nWHERE is_anomaly = true\n  AND timestamp > now() - interval '24 hours'\n  AND ( '$device' = 'ALL' OR device_id = '$device' )\nGROUP BY device_id\nORDER BY anomaly_count DESC;"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": { "align": "auto" }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "anomaly_count" },
            "properties": [
              {
                "id": "color",
                "value": { "mode": "continuous-RdYlGn" }
              }
            ]
          }
        ]
      }
    },
    {
      "id": 9,
      "type": "heatmap",
      "title": "Z-score heatmap (device vs time)",
      "datasource": "FLEAD-TimescaleDB",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 20 },
      "targets": [
        {
          "refId": "A",
          "format": "table",
          "rawSql": "SELECT\n  time_bucket('1 hour', timestamp) AS time,\n  device_id,\n  AVG(z_score) AS value\nFROM stream_analysis_results\nWHERE $__timeFilter(timestamp)\n  AND ( '$device' = 'ALL' OR device_id = '$device' )\nGROUP BY 1, device_id\nORDER BY 1, device_id;"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "options": {
        "legend": { "showLegend": true },
        "tooltip": { "show": true }
      }
    },
    {
      "id": 10,
      "type": "table",
      "title": "Batch analysis results (recent)",
      "datasource": "FLEAD-TimescaleDB",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 28 },
      "targets": [
        {
          "refId": "A",
          "format": "table",
          "rawSql": "SELECT\n  device_id,\n  metric_name,\n  avg_value,\n  min_value,\n  max_value,\n  stddev_value,\n  sample_count,\n  analysis_timestamp\nFROM batch_analysis_results\nWHERE $__timeFilter(analysis_timestamp)\n  AND ( '$device' = 'ALL' OR device_id = '$device' )\nORDER BY analysis_timestamp DESC\nLIMIT 200;"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": { "align": "auto" }
        },
        "overrides": []
      }
