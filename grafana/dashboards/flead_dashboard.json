  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Total anomalies (last 1h)",
      "datasource": "FLEAD-TimescaleDB",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
      "targets": [
        {
          "refId": "A",
          "format": "time_series",
          "rawSql": "SELECT\n  now() AS time,\n  COUNT(*) AS value\nFROM stream_analysis_results\nWHERE is_anomaly = true\n  AND timestamp > now() - interval '1 hour'\n  AND ( '$device' = 'ALL' OR device_id = '$device' );"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "orange", "value": 10 },
              { "color": "red", "value": 50 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "horizontal",
        "textMode": "value",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center"
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Avg Z-score (last 5m)",
      "datasource": "FLEAD-TimescaleDB",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 },
      "targets": [
        {
          "refId": "A",
          "format": "time_series",
          "rawSql": "SELECT\n  now() AS time,\n  AVG(z_score) AS value\nFROM stream_analysis_results\nWHERE timestamp > now() - interval '5 minutes'\n  AND ( '$device' = 'ALL' OR device_id = '$device' );"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "orange", "value": 1.5 },
              { "color": "red", "value": 3.0 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "horizontal",
        "textMode": "value",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center"
      }
    },
    {
      "id": 3,
      "type": "gauge",
      "title": "Latest Global Accuracy (%)",
      "datasource": "FLEAD-TimescaleDB",
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 },
      "targets": [
        {
          "refId": "A",
          "format": "time_series",
          "rawSql": "SELECT\n  created_at AS time,\n  accuracy * 100 AS value\nFROM federated_models\nORDER BY created_at DESC\nLIMIT 1;"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": 0 },
              { "color": "orange", "value": 70 },
              { "color": "green", "value": 85 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      }
    },
    {
      "id": 4,
