        "graphMode": "none",
        "justifyMode": "center"
      }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Anomalies over time",
      "datasource": "FLEAD-TimescaleDB",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "targets": [
        {
          "refId": "A",
          "format": "time_series",
          "rawSql": "SELECT\n  time_bucket('1 minute', timestamp) AS time,\n  COUNT(*) AS value\nFROM stream_analysis_results\nWHERE is_anomaly = true\n  AND $__timeFilter(timestamp)\n  AND ( '$device' = 'ALL' OR device_id = '$device' )\nGROUP BY 1\nORDER BY 1;"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "lineWidth": 2, "fillOpacity": 20 }
        },
        "overrides": []
      },
      "options": {
        "legend": { "showLegend": false },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Global model accuracy trend",
      "datasource": "FLEAD-TimescaleDB",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "targets": [
        {
          "refId": "A",
          "format": "time_series",
          "rawSql": "SELECT\n  created_at AS time,\n  accuracy * 100 AS value\nFROM federated_models\nWHERE $__timeFilter(created_at)\nORDER BY created_at;"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": { "mode": "palette-classic" },
