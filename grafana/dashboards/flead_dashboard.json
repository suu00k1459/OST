      "datasource": "FLEAD-TimescaleDB",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "targets": [
        {
          "refId": "A",
          "format": "time_series",
          "rawSql": "SELECT\n  time_bucket('1 minute', ts) AS time,\n  AVG(value) AS value\nFROM iot_data\nWHERE $__timeFilter(ts)\n  AND ( '$device' = 'ALL' OR device_id = '$device' )\nGROUP BY 1\nORDER BY 1;"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
